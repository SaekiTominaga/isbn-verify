{
	"name": "@saekitominaga/isbn-verify",
	"version": "1.0.2",
	"description": "Verify ISBN string format and check digit.",
	"type": "module",
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/plugin-transform-modules-commonjs": "^7.12.1",
		"@babel/preset-env": "^7.12.11",
		"@typescript-eslint/eslint-plugin": "^4.10.0",
		"@typescript-eslint/parser": "^4.10.0",
		"babel-jest": "^26.6.3",
		"eslint": "^7.15.0",
		"eslint-config-prettier": "^7.0.0",
		"eslint-plugin-jest": "^24.1.3",
		"eslint-plugin-jsdoc": "^30.7.8",
		"jest": "^26.6.3",
		"npm-run-all": "^4.1.5",
		"onchange": "^7.1.0",
		"renamer": "^2.0.1",
		"typescript": "^4.1.3"
	},
	"main": "ISBN.mjs",
	"types": "ISBN.mjs.d.ts",
	"scripts": {
		"_ts-watch:tsc": "tsc -d -w",
		"_ts-watch:jsrename": "onchange -v *.mjs.js -- renamer --force --find .mjs.js --replace .mjs *",
		"ts-watch": "run-p _ts-watch:*",
		"test": "jest"
	},
	"jest": {
		"transform": {
			"\\.(js|mjs|ts)$": "babel-jest"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SaekiTominaga/isbn-verify.git"
	},
	"keywords": [
		"ISBN"
	],
	"author": "Saeki Tominaga",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/SaekiTominaga/isbn-verify/issues"
	},
	"homepage": "https://github.com/SaekiTominaga/isbn-verify#readme"
}
